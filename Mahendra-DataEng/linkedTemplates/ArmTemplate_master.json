{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Mahendra-DataEng"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"LocalServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LocalServer'"
		},
		"LocalServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DELL"
		},
		"LocalServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "master"
		},
		"LocalServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DELL\\2025"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Mahendra-DataEng_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"LocalServer_password": {
						"value": "[parameters('LocalServer_password')]"
					},
					"LocalServer_properties_typeProperties_server": {
						"value": "[parameters('LocalServer_properties_typeProperties_server')]"
					},
					"LocalServer_properties_typeProperties_database": {
						"value": "[parameters('LocalServer_properties_typeProperties_database')]"
					},
					"LocalServer_properties_typeProperties_userName": {
						"value": "[parameters('LocalServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}